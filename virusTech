//Nicky Hall
//virusTech

solid(0,0.3,0.5)
  .mask(shape(3))
  .rotate(0.1,0.1)
  .mask(shape(5,0.5))
  .pixelate(20,10)
  .repeat(3,2)
  .blend(shape(3,0.5,0.5).rotate(0.5,0.3))
  .add(shape(5,0.2,0.3).pixelate(10,10).rotate(0.3,0.3).colorama(0.6).repeat(2,1).scrollX(0.1,0.1))

  .out()
